<section class="participants">
    <h1 class="title">Family Friday Participants</h1>

    <div class="body">
        <ul>
            <% @participants.each do |participant| %>
                <li><%= participant.name %></li>
            <% end %>
        </ul>
        <div class="submit">
            <button><a href="<%= generate_groups_participants_url %>">Mix It Up! <i class="fas fa-random"></i></a></button>
        </div>
    </div>
</section>